#%RAML 0.8
title: 测试
baseUri: http://localhost:80
version: 1.0

traits:
  - success:
      responses:
        200:
          description: 业务处理成功 code=0,否则code不为0
          body:
            application/json:
              example: |
                {
                  "code": "0",
                  "msg": "code不为0时的信息"
                }
              schema: |
                {
                  "$schema": "http://json-schema.org/draft-04/schema#",
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "string",
                      "description": "响应编码,0=成功,其他业务错误",
                      "example": "0"
                    },
                    "msg": {
                      "type": "string",
                      "description": "响应错误信息,code不等于0时有值",
                      "example": "用户已存在"
                    }
                  },
                  "required": [
                    "code"
                  ]
                }

/app:
