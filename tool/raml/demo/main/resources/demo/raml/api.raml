  description: APP
  /get:
    displayName: get方式请求
    description: get方式请求
    /noparam:
      displayName: noparam
      description: 没有参数
      get:
       is:
         - success:
    
    /uriparam/{id}:
      displayName: uriparam
      description: URI参数
      
      uriParameters:
        id:
          displayName: id
